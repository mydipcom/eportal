@using System.Data;
@using MS.ECP.BLL;
@using MS.ECP.Common;
@model MS.ECP.AAMAPrd.WebPager.PagedList<MS.ECP.Model.SysResource> 
 @section scripts
 {
    <script  type="text/javascript"  src="/Scripts/webpager.js"></script>
     <script type="text/javascript">
         jQuery(function () {
             jQuery(".listtr").each(function (i) {
                 var mod = (i + 2) % 2;

                 if (mod == 0) {
                     jQuery(this).children('th').addClass("spec");
                 } else {
                     jQuery(this).children('th').addClass("specalt");
                     jQuery(this).children("td").addClass("alt");
                 }
             });

         });
     </script>
 }


    <form runat="server" id="myfrm">
        <div>
            <div>
                <div class="content_block">
                    <h2 class="jquery_tab_title">WebSite Settings</h2>
                    <div id="resourcediv">
                        @Html.Partial("_ResourcePartialPage",Model)
                    </div>
                </div>
                <!--end content_block-->
            </div>
            <!--end jquery tab-->
        </div>
        <!--end content-->
    </form>

