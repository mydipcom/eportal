@{
    Layout = null;
}
<link type="text/css" href="/Content/zh-cn/css/newsdetail.css" rel="stylesheet">
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@model MS.ECP.AAMAPrd.Models.JoinPart

          @using (Html.BeginForm("JoinPary", "Home", FormMethod.Post))
          {
              <div class="listform">
                  <dl>
                      <dd>
                          <div class="reg_tit">
                              @Html.LabelFor(m => m.UserName)：
                          </div>
                          <div class="reg_con">
                              @Html.TextBoxFor(m => m.UserName, new {@class = "ipt_w"})
                              <div class="red_mark">*</div>
                              @Html.ValidationMessageFor(m => m.UserName)
                          </div>
                      </dd>
                      <dd>
                          <div class="reg_tit">
                              @Html.LabelFor(m => m.Sex)：
                          </div>

                          <div class="reg_con">
                              @Html.TextBoxFor(m => m.Sex, new {@class = "ipt_w"})                                       
                          </div>
                      </dd>
                      <dd>
                          <div class="reg_tit">
                              @Html.LabelFor(m => m.WorkPlace)：
                               
                          </div>
                          <div class="reg_con">
                              @Html.TextBoxFor(m => m.WorkPlace, new {@class = "ipt_w"})               
                          </div>
                      </dd>

                      <dd>
                          <div class="reg_tit">
                              @Html.LabelFor(m => m.Position)：
                          </div>

                          <div class="reg_con">
                              @Html.TextBoxFor(m => m.Position, new {@class = "ipt_w"})          
                          </div>
                      </dd>

                      <dd>
                          <div class="reg_tit">
                              @Html.LabelFor(m => m.Adress) ：
                          </div>
                          <div class="reg_con">
                              @Html.TextBoxFor(m => m.Adress, new {@class = "ipt_w"})          
                          </div>
                      </dd>
                      <dd>
                          <div class="reg_tit">
                              @Html.LabelFor(m => m.Phone)：      
                          </div>
                          <div class="reg_con">
                              @Html.TextBoxFor(m => m.Phone, new {@class = "ipt_w"})          
                          </div>
                      </dd>
                      <dd>
                          <div class="reg_tit">
                              @Html.LabelFor(m => m.MobliePhone)：                         
                          </div>
                          <div class="reg_con">
                              @Html.TextBoxFor(m => m.MobliePhone, new {@class = "ipt_w"})          
                          </div>
                      </dd>
                      <dd>
                          <div class="reg_tit">
                              @Html.LabelFor(m => m.Email)：                         
                          </div>
                          <div class="reg_con">
                              @Html.TextBoxFor(m => m.Email, new {@class = "ipt_w"}) 
                              <div class="red_mark">*</div>
                              @Html.ValidationMessageFor(m => m.Email)
                          </div>
                      </dd>
                      <dd>
                          <div class="reg_tit">
                              @Html.LabelFor(m => m.OthersPosition)：                            
                          </div>
                          <div class="reg_con">
                              @Html.TextAreaFor(m => m.OthersPosition, new {rows = "5", cols = "35"})
                          </div>
                      </dd>
                      <dd>
                          <div class="reg_tit">
                              @Html.LabelFor(m => m.ReMark) ：                       
                          </div>
                          <div class="reg_con">
                              @Html.TextAreaFor(m => m.ReMark, new {rows = "5", cols = "35"})
                          </div>
                      </dd>
                      <dd>

                          <div class="reg_tit">
                          </div>
                          <div class="reg_con">
                              <div style="width: 300px">
                                  <div style="float: left; margin-top: 5px;">
                                      <input name="chkUseYHQ" type="checkbox" onclick=" UseYHQ(); " id="useYHQ" value="0" /></div>
                                  <div style="float: left; padding-bottom: 15px; text-align: center; margin-right: 10px; margin-left: 10px">
                                      <h6>  @Html.LabelFor(m => m.Coupon)</h6>
                                  </div>
                                  <div style="float: left; display: none" id="divYHQ">
                                      <input id="txtYHQ" name="YHQ" type="text" style="width: 180px" /></div>
                              </div>
                          </div>
                      </dd>
                      <dd>
                          @Html.AntiForgeryToken()
                          @Html.ValidationSummary(true)
                      </dd>
                      <dd>
                          <div class="reg_tit"></div>
                          <div class="reg_con">
                              <div class="btn_t">
                                  <a> <input  name="submit" type="submit" value="提交"   style="width: 200px; height: 34px; border: 0px; background-color: transparent; font-size: 13px; color: white;cursor:pointer"/> </a>
                              </div>
                          </div>
                      </dd>
                  </dl>
              </div>
          }
